 <section class="column-9">
  <table>
    <tr>
      <th>No Ident</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Address</th>
      <th>Phone</th>
    </tr>

    <%= for t <- @terceros do %>
      <tr>
        <td><%= t.noident %></td>
        <td><%= t.first_name%></td>
        <td><%= t.last_name%></td>
        <td><%= t.email%></td>
        <td><%= t.dir%></td>
        <td><%= t.tel%></td>
      </tr>
    <% end %>
  </table>
</section>