<%= form_for @changeset, Routes.usuario_path(@conn, :create), fn f -> %>
  <label>
    Tercero id: <%= text_input f, :tercero_id%>
  </label>
  
  <label>
    Name User: <%= text_input f, :name_user %>
  </label>

  <label>
    Password: <%= hidden_input f, :password%>
  </label>

  
  <%= submit "Submit" %>
<% end %>